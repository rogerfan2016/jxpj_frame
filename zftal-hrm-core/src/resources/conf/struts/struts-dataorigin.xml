<struts>
    <package name="dataorigin" namespace="/dataorigin" extends="configuration">
      <action name="dataorigin_*" class="dataOriginAction" method="{1}" >
         <result name="page">/WEB-INF/pages/hrm/dataorigin/dataorigin_page.jsp</result>
         <result name="modify">/WEB-INF/pages/hrm/dataorigin/dataorigin_modify.jsp</result>
         <result name="userdefined">/WEB-INF/pages/hrm/dataorigin/dataorigin_userdefined.jsp</result>
         <result name="choiceproperty">/WEB-INF/pages/hrm/dataorigin/dataorigin_choiceproperty.jsp</result>
         <result name="addproperty">/WEB-INF/pages/hrm/dataorigin/dataorigin_addproperty.jsp</result>
      </action>
    </package>
</struts>